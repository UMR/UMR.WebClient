  <appSettings>
    <add key="DataSource" value="(DESCRIPTION = (ADDRESS_LIST = (ADDRESS = (PROTOCOL = TCP)(HOST = 172.16.205.66)(PORT = 1521)) )(CONNECT_DATA = (SERVICE_NAME = umrdb) ))"/>
    <add key="ProxyUserID" value="proxyuser"/>
    <add key="ProxyPassword" value="proxyuser"/>
    <add key="SmtpServer" value="smtp.gmail.com"/>
    <add key="SmtpPort" value="587"/>
    <add key="MailFrom" value="sccohnmd@gmail.com"/>
    <add key="SenderUserID" value="sccohnmd@gmail.com"/>
    <add key="SenderPassword" value="steven007"/>
    <add key="DisplayName" value="UMR System"/>
